  "bugs": {
    "url": "https://github.com/ThaUnknown/jassub/issues"
  },
  "homepage": "https://github.com/ThaUnknown/jassub",
  "dependencies": {
    "rvfc-polyfill": "^1.0.4"
  },
  "devDependencies": {
    "vite": "^4.1.4",
    "vite-plugin-static-copy": "^0.13.1"
  }
